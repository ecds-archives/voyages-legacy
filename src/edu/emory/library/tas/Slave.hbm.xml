<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="edu.emory.library.tas.Slave" table="slaves">
        <id name="iid" column="iid">
        	<generator class="native"/>
        </id>
        <property name="slaveId" column="sid"/>
        <property name="name" column="slave_name"/>
    </class>

</hibernate-mapping>