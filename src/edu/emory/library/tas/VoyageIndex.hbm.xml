<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="edu.emory.library.tas.VoyageIndex" table="voyages_index">
        <id name="id" column="voyage_index_id">
        	<generator class="native"/>
        </id>
        <property name="revisionDate" column="revision_date"/>
        <property name="flag" column="flag"/>
        <property name="voyageId" column="vid"/>
        <property name="revisionId" column="global_rev_id"/>
        
        <many-to-one name="voyage" class="edu.emory.library.tas.Voyage" column="r_voyage_iid" lazy="false" />
        <set name="slaves" table="voyage_slaves" lazy="false">
        	<key column="r_voyage_index_id"/>
        	<many-to-many column="r_slave_id" class="edu.emory.library.tas.Slave"/>
    	</set>
        
    </class>

</hibernate-mapping>