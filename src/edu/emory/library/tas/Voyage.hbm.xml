<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="edu.emory.library.tas.Voyage" table="voyages">
        <id name="iid" column="iid">
        	<generator class="native"/>
        </id>       
        <property name="voyageId" column="vid"/>
        <property name="shipname" column="ship_name"/>
        <property name="captainA" column="captaina"/>                        
        <property name="captainB" column="captainb"/>
        <property name="captainC" column="captainc"/>
        <many-to-one name="portdep" class="PortLocation" column="portdep" unique="true" lazy="false"/>
        <property name="datedepC" column="datedepc"/>
        <property name="datedepB" column="datedepb"/>
        <property name="datedepA" column="datedepa"/>
        <property name="datepl" column="datepl"/>
        
    </class>
    
</hibernate-mapping>