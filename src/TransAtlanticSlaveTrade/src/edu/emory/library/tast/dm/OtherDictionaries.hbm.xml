    </class>
    
    <class name="edu.emory.library.tast.dm.FateVessel" table="fates_vessel">
		<!-- 
		<cache usage="read-write" />
		-->
        <id name="id">
        	<generator class="native"/>
        </id>
        <property name="name" column="name"/>
