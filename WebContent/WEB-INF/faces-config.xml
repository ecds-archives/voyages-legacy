	<navigation-rule>
		<from-view-id>/schema-group-list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/schema-group-edit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/schema-group-edit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/schema-group-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/schema-attribute-list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/schema-attribute-edit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/schema-attribute-edit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/schema-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/schema-compound-attribute-list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/schema-compound-attribute-edit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/schema-compound-attribute-edit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/schema-compound-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>voyages-groups</from-outcome>
			<to-view-id>/schema-group-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>slaves-groups</from-outcome>
			<to-view-id>/schema-group-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>voyages-attributes</from-outcome>
			<to-view-id>/schema-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>slaves-attributes</from-outcome>
			<to-view-id>/schema-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>voyages-compound-attributes</from-outcome>
			<to-view-id>/schema-compound-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>slaves-compound-attributes</from-outcome>
			<to-view-id>/schema-compound-attribute-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/import-list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>detail</from-outcome>
			<to-view-id>/import-detail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

