	<navigation-rule>
		<from-view-id>/schema-group-list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/schema-group-edit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/schema-group-edit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/schema-group-list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/a.jsp</from-view-id>
		<navigation-case>
			<from-outcome>b</from-outcome>
			<to-view-id>/b.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/b.jsp</from-view-id>
		<navigation-case>
			<from-outcome>a</from-outcome>
			<to-view-id>/a.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

