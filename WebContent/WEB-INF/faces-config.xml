	<managed-bean>
		<managed-bean-name>SearchBean</managed-bean-name>
		<managed-bean-class>
			edu.emory.library.tas.web.SearchBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
